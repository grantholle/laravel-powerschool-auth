{
    "name": "grantholle/laravel-powerschool-auth",
    "description": "Authenticate your Laravel app with PowerSchool",
    "type": "library",
    "keywords": ["powerschool", "powerschool plugin", "sso"],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "grantholle/pear-openid": "^1.0",
        "illuminate/support": "^8.0|^9.0",
        "illuminate/http": "^8.0|^9.0",
        "spatie/url": "^1.3|^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Grant Holle",
            "homepage": "https://grantholle.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "GrantHolle\\PowerSchool\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": "GrantHolle\\PowerSchool\\Auth\\PowerSchoolAuthServiceProvider"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "spatie/ray": "^1.33"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
